{
    "name": "ecarx-editor-app",
    "version": "1.0.0",
    "private": true,
    "keywords": [],
    "scripts": {
        "i": "yarn",
        "start-local": "cross-env PROXY_ENV=local webpack-dev-server --config build/webpack.dev.js",
        "start-demo": "cross-env PROXY_ENV=demo webpack-dev-server --config build/webpack.dev.js",
        "dev": "cross-env PROXY_ENV=dev webpack-dev-server --config build/webpack.dev.js",
        "build": "cross-env PROXY_ENV=prod webpack --config build/webpack.prod.js",
        "start": "npm run dev --ENV=local",
        "build:dev": "npm run build --ENV=dev",
        "build:pre": "npm run build --ENV=pre",
        "build:prd": "npm run build --ENV=prd",
        "build:test": "npm run build --ENV=test",
        "update-sdk": "yarn upgrade @ad/xmap@~1.0.7"
    },
    "sideEffects": [
        "@babel/polyfill",
        "*.css",
        "*.less"
    ],
