{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:compat/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": false
    }
  },
  "rules": {
    "no-console": "off",
    "strict": [
      "error",
      "global"
    ],
    "curly": "warn"
  }
}